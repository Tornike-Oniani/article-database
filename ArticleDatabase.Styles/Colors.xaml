<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Lib.Styles">


    <!-- Primary color -->
    <Color x:Key="PrimaryColor">#3094C0</Color>
    <SolidColorBrush x:Key="PrimaryColorBrush" Color="{StaticResource PrimaryColor}" />

    <!-- Secondary color -->
    <Color x:Key="SecondaryColor">#0575AC</Color>
    <SolidColorBrush x:Key="SecondaryColorBrush" Color="{StaticResource SecondaryColor}" />

    <!-- Complementary color -->
    <Color x:Key="ComplementaryColor">#f07c41</Color>
    <SolidColorBrush x:Key="ComplementaryColorBrush" Color="{StaticResource ComplementaryColor}" />

    <!-- Highlight color -->
    <Color x:Key="HighlightColor">#f0c748</Color>
    <SolidColorBrush x:Key="HighlightColorBrush" Color="{StaticResource HighlightColor}" />

    <!-- Light highlight color -->
    <Color x:Key="LightHighlightColor">#e5f3fb</Color>
    <SolidColorBrush x:Key="LightHighlightColorBrush" Color="{StaticResource LightHighlightColor}" />
    
    <!-- Light selection color -->
    <Color x:Key="LightSelectionColor">#cbe8f6</Color>
    <SolidColorBrush x:Key="LightSelectionColorBrush" Color="{StaticResource LightSelectionColor}" />

    <!-- Control text color -->
    <Color x:Key="ControlColor">#ffffff</Color>
    <SolidColorBrush x:Key="ControlColorBrush" Color="{StaticResource ControlColor}" />

    <!-- Primary color new -->
    <Color x:Key="PrimaryColorNew">#458dbd</Color>
    <SolidColorBrush x:Key="PrimaryColorNewBrush" Color="{StaticResource PrimaryColorNew}" />
    <!-- Primary color new dark -->
    <Color x:Key="PrimaryColorNewDark">#2375af</Color>
    <SolidColorBrush x:Key="PrimaryColorNewDarkBrush" Color="{StaticResource PrimaryColorNewDark}" />
    <!-- Primary color new light -->
    <Color x:Key="PrimaryColorNewLight">#509dd3</Color>
    <SolidColorBrush x:Key="PrimaryColorNewLightBrush" Color="{StaticResource PrimaryColorNewLight}" />
    <!-- Gray color light -->
    <Color x:Key="GrayColorLight">#dedfe1</Color>
    <SolidColorBrush x:Key="GrayColorLightBrush" Color="{StaticResource GrayColorLight}" />
    <!-- Gray color light -->
    <Color x:Key="GrayColorLight--1">#f0f2f5</Color>
    <SolidColorBrush x:Key="GrayColorLightBrush--1" Color="{StaticResource GrayColorLight--1}" />
    <!-- Label color -->
    <Color x:Key="LabelColor">#35495E</Color>
    <SolidColorBrush x:Key="LabelColorBrush" Color="{StaticResource LabelColor}" />
    <!-- Label color light -->
    <Color x:Key="LabelColorLight">#35495E</Color>
    <SolidColorBrush x:Key="LabelColorLightBrush" Color="{StaticResource LabelColor}" Opacity="0.66" />
    <!-- Label color light -->
    <Color x:Key="LabelColorSemiLight">#35495E</Color>
    <SolidColorBrush x:Key="LabelColorSemiLightBrush" Color="{StaticResource LabelColorSemiLight}" Opacity="0.8" />


</ResourceDictionary>